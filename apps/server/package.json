  "license": "ISC",
  "description": "",
  "dependencies": {
    "@clerk/express": "^1.3.52",
    "@cmumaps/common": "workspace:*",
    "@prisma/client": "^6.8.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.11.24",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.23.0",
    "eslint": "^9.23.0",
    "eslint-plugin-import": "^2.31.0",
