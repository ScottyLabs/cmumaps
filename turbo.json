{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:host": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": [
        "CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "ALLOWED_ORIGINS_REGEX",
        "DATABASE_URL",
        "VITE_MAPKIT_TOKEN",
        "VITE_SERVER_URL",
        "VITE_RUST_SERVER_URL",
        "VITE_CLERK_PUBLISHABLE_KEY",
        "VITE_PUBLIC_POSTHOG_HOST",
        "VITE_PUBLIC_POSTHOG_KEY"
      ]
    },
    "start": {
      "dependsOn": ["^start", "^build"],
      "persistent": true
    },
    "format": {},
    "transit": {
      "dependsOn": ["^transit"]
    },
    "lint": {
      "dependsOn": ["transit"]
    },
    "tsc": {
      "dependsOn": ["transit"]
    },
    "check": {
      "dependsOn": ["transit"]
    }
  }
}
